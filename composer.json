{
    "name": "alexk/craft-react",
    "description": "Client and Server-side React rendering for CraftCMS",
    "version": "1.2.2",
    "type": "craft-plugin",
    "keywords": ["react", "reactjs", "isomorphic", "twig", "craft"],
    "require": {
      "craftcms/cms": "4.8.4",
      "limenius/react-renderer": "^5.0.4",
      "twig/twig": "v3.8.0"
    },
    "autoload": {
      "psr-4": {
        "react\\": "src/"
      }
    },
    "license": "MIT",
    "extra": {
      "handle": "react",
      "name": "React for Craft",
      "developer": "Alexandre Kilian",
      "developerUrl": "https://github.com/AlexandreKilian"
    },
    "config": {
        "allow-plugins": {
            "yiisoft/yii2-composer": true,
            "craftcms/plugin-installer": true
        }
    }
}
